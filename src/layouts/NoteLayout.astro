---
import type { CollectionEntry } from 'astro:content';
import BaseHead from '../components/BaseHead.astro';

type Props = CollectionEntry<'notes'>['data'];

const { title, description, date, } = Astro.props;
---

<html lang="en">
	<head>
		<BaseHead title={title} description="felix murray tang's notes" />
	</head>

	<body>
		<main>
			<article>
				<h1>{title}</h1>
				<slot />
			</article>
		</main>
	</body>
</html>
